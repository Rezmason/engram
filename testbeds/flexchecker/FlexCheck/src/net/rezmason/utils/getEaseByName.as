package net.rezmason.utils {		// IMPORT STATEMENTS	import flash.utils.getDefinitionByName;		public function getEaseByName(className:String):Object {				trace(isFlexCompiled());				var altName:String = "altName";				// convert Flex easing functions to Flash		switch (className) {			case "Linear":				altName = "None";			break;			case "Quintic":				altName = "Strong";			break;			case "Quadratic":				altName = "Regular";			break;			default:				altName = className;			break;		}				// try to find Flash easing functions		try {			return getDefinitionByName("fl.transitions.easing." + altName);		} catch (error:Error) {			trace("Not fl.");		}				altName = "altName";				// convert Flash easing functions to Flex		switch (className) {			case "None":				altName = "Linear";			break;			case "Regular":				altName = "Quadratic";			break;			case "Strong":				altName = "Quintic";			break;			default:				altName = className;			break;		}				// try to find Flex easing functions		try {			return getDefinitionByName("mx.effects.easing." + altName);		} catch (error:Error) {			trace("Not mx.");		}				return null;	}	}