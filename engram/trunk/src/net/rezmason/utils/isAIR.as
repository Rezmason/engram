package net.rezmason.utils {		import flash.system.Security;		/**	* 	Returns <code>true</code> if the currently running Flash instance	*	is an AIR application.	*		*	@return	If the running Flash instance is an AIR application,	*	this value is <code>true</code>, otherwise <code>false</code>.	*	*/	public function isAIR():Boolean {				return (Security.sandboxType == "application");			}}