package net.rezmason.engram.games.busybodhi {	// IMPORT STATEMENTS	import flash.display.Sprite;	public class TrackSprite extends Sprite {		public function TrackSprite():void {			with (graphics) {				beginFill(0xFFFFFF);				drawRect(0, -20, 2000, 40);				endFill();			}		}				internal function land():void {			trace("I have landed!");		}				internal function die():void {			trace("I am dead!");		}	}}