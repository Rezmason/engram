package net.rezmason.engram {		// IMPORT STATEMENTS	import flash.events.Event;		public final class ModuleLoaderEvent extends Event {				// CLASS PROPERTIES		internal static const DONE:String = "done", NEXT:String = "next";		internal static const FAIL:String = "fail";				// INSTANCE PROPERTIES		private var _timeOut:Boolean;				// CONSTRUCTOR		public function ModuleLoaderEvent(type:String, timeout:Boolean = false,		bubbles:Boolean = true, cancelable:Boolean = false):void {			super(type, bubbles, cancelable);			_timeOut = timeout;		}				// GETTERS & SETTERS				public function get timeOut() : Boolean {			return _timeOut;		}				public function set timeOut(value:Boolean):void {			_timeOut = value;		}				// PUBLIC METHODS				override public function clone() : Event {			return new ModuleLoaderEvent(type, bubbles, cancelable);		}	}	}