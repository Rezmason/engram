package net.rezmason.engram.menus {		// IMPORT STATEMENTS	import flash.events.Event;	import flash.events.MouseEvent;		import net.rezmason.engram.View;		public final class MainMenu extends MenuBase {						public function MainMenu(__view:View):void {			btnNewGame.addEventListener(MouseEvent.CLICK, __view.showGameMenu);			btnSettings.addEventListener(MouseEvent.CLICK, __view.showSettings);			btnAbout.addEventListener(MouseEvent.CLICK, __view.showAboutBox);			btnHighScores.addEventListener(MouseEvent.CLICK, __view.showScoreboard);			glowTraceLogo.addEventListener(Event.COMPLETE, __view.showDebugButton);			//logo.gotoAndStop("end");		}	}}