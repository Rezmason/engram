stop();import net.rezmason.display.Grid;import net.rezmason.display.GridFill;var grid:Grid = new Grid();grid.fill = [GridFill.LEFT_TO_RIGHT, GridFill.TOP_TO_BOTTOM];grid.spacing = 10;grid.cellWidth = 40;grid.maxWidth  = stage.stageWidth;grid.maxHeight = stage.stageHeight;addChild(grid);var ike:int;for (ike = 0; ike < 50; ike++) {	element = new Element;	element.alpha = ike / 50 + 0.1;	grid.place(element);}	var itr:int = 0;var element:Element;stage.addEventListener(MouseEvent.MOUSE_MOVE, update);function update(event:Event):void {	grid.cellWidth = mouseX;}function findElementAND(n:uint):Boolean {	var ones:int = 0;		while (n > 0) {		ones += (n & 1);		n >>>= 1;	}		return !!(ones & 1);}