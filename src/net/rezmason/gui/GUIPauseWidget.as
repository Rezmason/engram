package net.rezmason.gui {		// IMPORT STATEMENTS	import flash.display.Stage;		public class GUIPauseWidget extends GUIWindowWidget {				// CONSTRUCTOR		public function GUIPauseWidget():void {			super();						try {				back;			} catch (error:Error) {				throw new GUIError(this, "back", "InteractiveObject");			}		}				// PUBLIC METHODS				override public function init(offset:int):void {						_pauseButton = back;						super.init(offset);		}				override public function addToStage(stg:Stage):void {			stg.addChild(this);						x = y = _offset;		}	}}