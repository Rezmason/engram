package net.rezmason.utils {		// IMPORT STATEMENTS	import flash.utils.getDefinitionByName;		/**	*	Uses a call to the <b>Adobe AIR Application Installer</b> 	*	to launch a specified application. 	*		*	</p>You must provide the applicationID and publisherID of the app	*	you intend to launch; this information can be found in the files	*	within the target AIR application. Your user may point out the	*	target app for you, whereupon you can extract the information	*	necessary to launch the it.</p>	*		*	@param	appID	 The application ID of the target AIR app	*	@param	publisherID	The publisher ID of the target AIR app	*	*	@return		If ActionScript errors occur with the function, it returns	*	<code>false</code>. Otherwise, it returns <code>true</code>.	*/	public function bootInstalledAIRApp(appID:String, publisherID:String):Boolean {		var PM:Class;		try {			PM = getDefinitionByName("adobe.utils.ProductManager") as Class;			var pmgr:* = new PM("airappinstaller");			if (pmgr.installed) {				pmgr.launch("-launch " + appID + " " + publisherID);				return true;			} else {				return false;			}		} catch (error:Error) {			trace("boot failed.");		}		return false;	}}