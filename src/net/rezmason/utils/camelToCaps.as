package net.rezmason.utils {		/**	* Converts a <code>String</code> that is in camelCase to one that is in ALL CAPS WITH SPACES.	*		*	<p>A space is inserted between any pair of letters, where the first is lower case	*	and the last is upper case. This method is useful for converting the names	*	of interface elements to labels.</p>	*		*	@param	str	 Input <code>String</code>.	*	@return		A <code>String</code> containing spaces and no lower case characters.	*/	public function camelToCaps(str:String):String {		return str.replace(/([a-z])([A-Z])([A-Z])([a-z])/g, "$1$2 $3$4").replace(/([a-z])([A-Z])/g, "$1 $2").toUpperCase();	}}