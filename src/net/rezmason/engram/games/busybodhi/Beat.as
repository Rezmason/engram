package net.rezmason.engram.games.busybodhi {	internal class Beat {				// INSTANCE PROPERTIES		public var key:String;		public var time:int;		public function Beat(__key:String = "", __time:int = 0):void {			key = __key;			time = __time;		}				public function toString():String {			return "# " +  key + " " + time.toString() + " #";		}	}}