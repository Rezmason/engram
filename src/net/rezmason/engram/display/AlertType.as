package net.rezmason.engram.display {		// IMPORT STATEMENTS		import flash.display.DisplayObject;	public final class AlertType {				// CLASS PROPERTIES				public static const BUG:AlertType = new AlertType(0, "BugSymbol", 1);		public static const CANNOT_UNDO:AlertType = new AlertType(2, "CannotUndoSymbol", 0.4);		public static const DECISION:AlertType = new AlertType(2, "DecisionSymbol", 0.4);		public static const DEVELOPER:AlertType = new AlertType(1, "DeveloperSymbol", 1);		public static const PROBLEM:AlertType = new AlertType(0, "ProblemSymbol", 1);		public static const WARNING:AlertType = new AlertType(1, "WarningSymbol", 1);				// INSTANCE PROPERTIES				public var symbol:*;		public var paletteIndex:uint;		public var textureAlpha:Number = 0;						public function AlertType(__paletteIndex:uint = 0, __symbol:* = null, __textureAlpha:Number = 1):void {			symbol = __symbol;			paletteIndex = __paletteIndex;			textureAlpha = __textureAlpha;		}	}}